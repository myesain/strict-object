    , "keywords"    : ["debug", "debugger", "OOP", "reliability", "exception"]
    , "homepage"    : "https://github.com/myesain/strict-object"
    , "license"     : "MIT"
    , "authors"     : [
        {
              "name"     : "Noah Heck"
            , "role"     : "Developer/Project Manager"
            , "email"    : "myesain84@gmail.com"
            , "homepage" : "https://github.com/myesain"
        }
    ]
    , "require"     : {
        "php"                   : ">=5.4.0"
    }
    , "require-dev" : {
          "phpunit/phpunit"     : "4.8.*"
